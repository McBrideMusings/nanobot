{
  "providers": {
    "vllm": {
      "apiKey": "dummy",
      "apiBase": "http://100.114.249.118:8000/v1"
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.nanobot/workspace",
      "model": "local-model",
      "maxTokens": 4096,
      "temperature": 0.7,
      "maxToolIterations": 20
    }
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "YOUR_TELEGRAM_BOT_TOKEN_HERE",
      "allowFrom": []
    },
    "api": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 18790
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  },
  "tools": {
    "restrictToWorkspace": false,
    "web": {
      "search": {
        "provider": "brave",
        "braveApiKey": "YOUR_BRAVE_API_KEY_HERE",
        "tavilyApiKey": "YOUR_TAVILY_API_KEY_HERE",
        "maxResults": 5
      }
    }
  }
}
